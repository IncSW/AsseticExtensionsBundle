<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="assetic_extensions_bundle.assetic.filter.babel" class="IncSW\AsseticExtensionsBundle\Assetic\Filter\BabelFilter">
            <tag name="assetic.filter" alias="babel"/>
            <argument>%assetic.filter.babel.bin%</argument>
            <argument>%assetic.node.bin%</argument>
            <argument>%assetic.filter.babel.config%</argument>
            <call method="setTimeout">
                <argument>null</argument>
            </call>
            <call method="setNodePaths">
                <argument>%assetic.node.paths%</argument>
            </call>
        </service>
    </services>
</container>
